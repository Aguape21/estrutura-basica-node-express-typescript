// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model contas {
  indez String @id @default(cuid())
  nome String? 
  email String?
  telefone String?
  atuacao String?
  origem String
  criacao DateTime @default(now())
}
